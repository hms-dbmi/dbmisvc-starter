{
    "branches": [
        "master",
        {"name": "development", "channel": "pre/rc", "prerelease": "rc"},
        {"name": "beta", "prerelease": true},
        {"name": "alpha", "prerelease": true}
    ],
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        ["@semantic-release/changelog", {
            "changelogFile": "CHANGELOG.md"
        }],
        ["@semantic-release/exec", {
          "prepareCmd": "sed -ie \"s/^__version__.*/__version__ = \\x22${nextRelease.version}\\x22/g\" starter/starter/__init__.py && sed -ie \"s/\\x22version\\x22: \\x22.*\\x22/\\x22version\\x22: \\x22${nextRelease.version}\\x22/g\" package.json"
        }],
        ["@semantic-release/git", {
            "assets": ["starter/starter/__init__.py", "CHANGELOG.md", "package.json", "package-lock.json"]
        }],
        ["@semantic-release/github", {
            "assets": ["*"]
        }]
    ],
    "preset": "angular"
}
